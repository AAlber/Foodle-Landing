{
  "name": "foodle",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Foodle",
  "private": "true",
  "scripts": {
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:reset": "dotenv -e .env.local prisma migrate reset",
    "prisma:generate": "dotenv -e .env.local prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate:deploy": "dotenv -e .env.local prisma migrate deploy",
    "dev": "dotenv -e .env.local -- ts-node-dev --no-notify --respawn --transpile-only server/index.ts",
    "nexus:watch": "nodemon --exec 'yarn ts-node server/schema' --ext 'ts' --watch 'server/graphql/**/*.ts' --ignore 'graphql/generated/schema.graphql'",
    "codegen:generate": "graphql-codegen",
    "db:seed": "dotenv -e .env.local yarn ts-node utils/runSeed.ts",
    "db:clean": "dotenv -e .env.local yarn ts-node utils/runClean.ts",
    "format": "eslint --fix .",
    "test:api": "cross-env TEST=true dotenv -e .env.local -- jest --config ./jest.config.ts --runInBand --force-exit",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "heroku:deploy": "yarn prisma migrate deploy && node build/server/index.js"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-query": "^3.5.5",
    "@hookform/resolvers": "^2.8.8",
    "@prisma/client": "^3.9.1",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/helmet": "^4.0.0",
    "apollo-server": "^3.6.2",
    "aws-amplify": "^4.3.14",
    "aws-sdk": "^2.1109.0",
    "express-session": "^1.17.2",
    "graphql": "14.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-scalars": "^1.16.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "next": "12.1.0",
    "nexus": "^1.2.0",
    "passport": "^0.5.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^12.0.4",
    "react-hook-form": "^7.27.1",
    "react-icons": "^4.3.1",
    "react-input-children": "^5.0.0",
    "react-query": "^3.34.16",
    "sass": "^1.49.7",
    "stripe": "^8.207.0",
    "uuid": "^8.3.2",
    "zod": "^3.12.0"
  },
  "devDependencies": {
    "@dotenv/cli": "^2.0.0",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.14",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/react": "17.0.38",
    "@types/stripe-v3": "^3.1.26",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "12.0.10",
    "express": "^4.17.2",
    "jest": "^27.4.7",
    "nexus-prisma": "^0.35.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.9.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
